>div.{{style.editor}}
	>div.{{style.container}}
		>input.{{style.title}}
			#placeholder = Title*
			%value = {{title}}
		>input.{{style.name}}
			#placeholder = name
			%value = {{name}}
		>textarea#editor
			%value = {{content}}
			.{{content}}
		>div.{{style.btns}}
			>span
				@click = cancel
				.Cancel
			>span
				@click = save:{{index}}
				.Save
